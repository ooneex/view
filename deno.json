{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "emitDecoratorMetadata": true,
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ]
  },
  "imports": {
    "@config/": "./config/",
    "@islands/": "./islands/",
    "@config/app": "./config/app/mod.ts",
    "@views": "./views/mod.ts",
    "@controllers/": "./controllers/",

    "@hypervit/collection": "https://deno.land/x/hypervit@0.0.14/Collection/mod.ts",
    "@hypervit/config": "https://deno.land/x/hypervit@0.0.14/Config/mod.ts",
    "@hypervit/controller": "https://deno.land/x/hypervit@0.0.14/Controller/mod.ts",
    "@hypervit/decorator": "https://deno.land/x/hypervit@0.0.14/Decorator/mod.ts",
    "@hypervit/directory": "https://deno.land/x/hypervit@0.0.14/Directory/mod.ts",
    "@hypervit/env": "https://deno.land/x/hypervit@0.0.14/Env/mod.ts",
    "@hypervit/exception": "https://deno.land/x/hypervit@0.0.14/Exception/mod.ts",
    "@hypervit/file": "https://deno.land/x/hypervit@0.0.14/File/mod.ts",
    "@hypervit/helper": "https://deno.land/x/hypervit@0.0.14/Helper/mod.ts",
    "@hypervit/http": "https://deno.land/x/hypervit@0.0.14/Http/mod.ts",
    "@hypervit/ioc": "https://deno.land/x/hypervit@0.0.14/Ioc/mod.ts",
    "@hypervit/island": "https://deno.land/x/hypervit@0.0.14/Island/mod.ts",
    "@hypervit/kernel": "https://deno.land/x/hypervit@0.0.14/Kernel/mod.ts",
    "@hypervit/output": "https://deno.land/x/hypervit@0.0.14/Output/mod.ts",
    "@hypervit/path": "https://deno.land/x/hypervit@0.0.14/Path/mod.ts",
    "@hypervit/routing": "https://deno.land/x/hypervit@0.0.14/Routing/mod.ts",
    "@hypervit/view": "https://deno.land/x/hypervit@0.0.14/View/mod.ts",

    "testing/": "https://deno.land/std@0.182.0/testing/",
    "preact": "https://esm.sh/preact@10.13.2",
    "preact/": "https://esm.sh/preact@10.13.2/",
    "solid-js": "https://cdn.skypack.dev/solid-js",
    "solid-js/web": "https://cdn.skypack.dev/solid-js/web"
  },
  "tasks": {
    "start": "deno run -A --watch=.env,.env.local,config/,controllers/,middlewares/,islands/ index.ts",
    "assets:watch": "docker-compose run --rm hypervit_view_assets sh ./watch.sh",
    "assets:build": "docker-compose run --build --rm hypervit_view_assets sh ./build.sh",
    "assets:sh": "docker-compose run --rm hypervit_view_assets sh",
    "test": "deno test -A",
    "test:watch": "deno test -A --watch",
    "fmt": "deno fmt",
    "lint": "deno fmt --check && deno lint"
  },
  "nodeModulesDir": true
}
