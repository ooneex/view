{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "emitDecoratorMetadata": true,
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ]
  },
  "imports": {
    "@components/": "./components/",
    "@config/": "./config/",
    "@config/app": "./config/app/mod.ts",
    "@controllers/": "./controllers/",
    "@islands/": "./islands/",
    "@public/": "./public/",
    "@tests/": "./tests/",
    "@var/": "./var/",
    "@views/": "./views/",

    "@hypervit/collection": "https://deno.land/x/hypervit@0.0.19/Collection/mod.ts",
    "@hypervit/config": "https://deno.land/x/hypervit@0.0.19/Config/mod.ts",
    "@hypervit/controller": "https://deno.land/x/hypervit@0.0.19/Controller/mod.ts",
    "@hypervit/decorator": "https://deno.land/x/hypervit@0.0.19/Decorator/mod.ts",
    "@hypervit/directory": "https://deno.land/x/hypervit@0.0.19/Directory/mod.ts",
    "@hypervit/env": "https://deno.land/x/hypervit@0.0.19/Env/mod.ts",
    "@hypervit/exception": "https://deno.land/x/hypervit@0.0.19/Exception/mod.ts",
    "@hypervit/file": "https://deno.land/x/hypervit@0.0.19/File/mod.ts",
    "@hypervit/helper": "https://deno.land/x/hypervit@0.0.19/Helper/mod.ts",
    "@hypervit/http": "https://deno.land/x/hypervit@0.0.19/Http/mod.ts",
    "@hypervit/ioc": "https://deno.land/x/hypervit@0.0.19/Ioc/mod.ts",
    "@hypervit/island": "https://deno.land/x/hypervit@0.0.19/Island/mod.ts",
    "@hypervit/kernel": "https://deno.land/x/hypervit@0.0.19/Kernel/mod.ts",
    "@hypervit/output": "https://deno.land/x/hypervit@0.0.19/Output/mod.ts",
    "@hypervit/path": "https://deno.land/x/hypervit@0.0.19/Path/mod.ts",
    "@hypervit/routing": "https://deno.land/x/hypervit@0.0.19/Routing/mod.ts",
    "@hypervit/testing": "https://deno.land/x/hypervit@0.0.19/Testing/mod.ts",
    "@hypervit/view": "https://deno.land/x/hypervit@0.0.19/View/mod.ts",

    "dom": "https://deno.land/x/deno_dom@v0.1.38/deno-dom-wasm.ts",
    "preact": "https://esm.sh/preact@10.13.2",
    "preact/": "https://esm.sh/preact@10.13.2/",
    "solid-js": "https://cdn.skypack.dev/solid-js",
    "solid-js/web": "https://cdn.skypack.dev/solid-js/web"
  },
  "tasks": {
    "start": "deno run -A --watch=.env,.env.local,config/,controllers/,middlewares/ index.ts",
    "assets:watch": "docker-compose run --rm hypervit_view_assets sh ./watch.sh",
    "assets:build": "docker-compose run --rm hypervit_view_assets sh ./build.sh",
    "assets:sh": "docker-compose run --rm hypervit_view_assets sh",
    "test": "deno test -A",
    "test:watch": "deno test -A --watch",
    "fmt": "deno fmt",
    "lint": "deno fmt --check && deno lint",
    "cache": "deno cache ./**/*.ts",
    "make:component": "deno run -A https://deno.land/x/ooneex@0.0.15/ooneex.ts make:component",
    "make:controller": "deno run -A https://deno.land/x/ooneex@0.0.15/ooneex.ts/ooneex.ts make:controller",
    "make:island": "deno run -A https://deno.land/x/ooneex@0.0.15/ooneex.ts/ooneex.ts make:island",
    "make:view": "deno run -A https://deno.land/x/ooneex@0.0.15/ooneex.ts/ooneex.ts make:view"
  },
  "lint": {
    "exclude": [
      "node_modules",
      "public/dist/",
      "var"
    ]
  },
  "fmt": {
    "exclude": [
      "node_modules",
      "public/dist",
      "var"
    ]
  },
  "lock": false,
  "nodeModulesDir": true
}
